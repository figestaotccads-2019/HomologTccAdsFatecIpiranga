android:label="@string/PerfilEditActivity">
</activity>

<activity android:name=".view.MapsActivity"
android:theme="@style/AppTheme3"
android:label="@string/MapsActivity">
</activity>

    <activity
android:name=".view.login.LoginActivity"
android:theme="@style/AppTheme3"
android:label="@string/LoginActivity">
</activity>

    <activity
        android:name=".view.perfil.PerfilActivity"
        android:theme="@style/AppTheme3"
        android:label="@string/PerfilActivity">
    </activity>

<meta-data
android:name="com.google.firebase.ml.vision.DEPENDENCIES"
android:value="barcode" />

</application>

</manifest>